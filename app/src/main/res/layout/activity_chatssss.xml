<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Chatssss">

    <androidx.constraintlayout.widget.Barrier
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:constraint_referenced_ids="textView29,view1" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chats"
        android:textSize="16dp"
        android:textColor="#A7A7A7"
        android:fontFamily="@font/roboto_medium"
        android:translationX="170dp"
        android:gravity="center"
        android:translationY="10dp"/>

    <View
        android:id="@+id/view1"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:translationY="0dp" />

    <ImageView
        android:id="@+id/back"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:translationX="15dp"
        android:translationY="10dp"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageView1000"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="25dp"
        android:translationY="150dp"
        app:srcCompat="@drawable/johnjoshua" />

    <ImageView
        android:id="@+id/imageView1001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="25dp"
        android:translationY="250dp"
        app:srcCompat="@drawable/chinonsojam" />

    <ImageView
        android:id="@+id/imageView1002"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="25dp"
        android:translationY="350dp"
        app:srcCompat="@drawable/ralph" />
    <ImageView
        android:id="@+id/imageView1003"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="25dp"
        android:translationY="450dp"
        app:srcCompat="@drawable/joy" />
    <ImageView
        android:id="@+id/imageView1004"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:translationX="25dp"
        android:translationY="550dp"
        app:srcCompat="@drawable/joyy" />

    <EditText
        android:id="@+id/editTextText13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint=" Search services"
        android:textColorHint="#A7A7A7"
        android:fontFamily="@font/roboto"
        android:background="@drawable/edit"
        android:inputType="text"
        android:translationY="80dp"
        android:translationX="25dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>