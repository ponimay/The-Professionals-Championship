<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Track">

    <androidx.constraintlayout.widget.Barrier
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        app:constraint_referenced_ids="view" />

    <ImageView
        android:id="@+id/homeeeeee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/houseeee"
        android:translationY="730dp"
        android:translationX="50dp"/>

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:textSize="12dp"
        android:textColor="#A7A7A7"
        android:translationX="45dp"
        android:translationY="755dp"/>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/poloska"
        android:translationY="720dp"
        android:translationX="245dp"/>

    <View
        android:id="@+id/view"
        android:layout_width="390dp"
        android:layout_height="50dp"
        android:translationY="720dp" />

    <ImageView
        android:id="@+id/wallet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/wallet"
        android:clickable="true"
        android:translationX="150dp"
        android:translationY="730dp"/>

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wallet"
        android:textSize="12dp"
        android:textColor="#A7A7A7"
        android:translationX="145dp"
        android:translationY="755dp"/>

    <ImageView
        android:id="@+id/track"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/carrr"
        android:translationY="730dp"
        android:translationX="250dp"/>

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Track"
        android:textSize="12dp"
        android:textColor="#0560FA"
        android:translationX="245dp"
        android:translationY="755dp"/>

    <ImageView
        android:id="@+id/prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:translationX="355dp"
        android:translationY="730dp"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/proftxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textSize="12dp"
        android:textColor="#A7A7A7"
        android:translationX="350dp"
        android:translationY="755dp"/>

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/mapapa"
        android:translationX="0dp"
        android:translationY="0dp"
        />

    <TextView
        android:id="@+id/textView76"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tracking Number"
        android:translationX="35dp"
        android:textColor="#3A3A3A"
        android:fontFamily="@font/roboto_medium"
        android:textSize="16dp"
        android:translationY="350dp"/>

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/koleso"
        android:translationY="390dp"
        android:translationX="35dp"/>

    <TextView
        android:id="@+id/textView77"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="R-7458-4567-4434-5854"
        android:translationX="55dp"
        android:textColor="#0560FA"
        android:fontFamily="@font/roboto"
        android:textSize="16dp"
        android:translationY="386dp"/>

    <TextView
        android:id="@+id/textView78"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Package Status"
        android:translationX="35dp"
        android:textColor="#A7A7A7"
        android:fontFamily="@font/roboto"
        android:textSize="14dp"
        android:translationY="420dp"/>

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:translationX="35dp"
        android:translationY="460dp"
        app:srcCompat="@drawable/galochka"
        />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:translationX="65dp"
        android:translationY="460dp"
        app:srcCompat="@drawable/text"
        />
    <Button
        android:id="@+id/NextBtn"
        android:layout_width="360dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#0560FA"
        android:translationX="20dp"
        android:translationY="660dp"
        android:textStyle="bold"
        android:text="View Package Info"
        app:cornerRadius="4dp" />

</androidx.constraintlayout.widget.ConstraintLayout>